<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hoyocon.bomberman.GameOverController">
   <children>
      <ImageView fitHeight="1080.0" fitWidth="1920.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@b0cf517597fd9ae3948d3181b1db2736.jpg" />
         </image>
      </ImageView>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="715.0" layoutY="252.0" prefHeight="233.0" prefWidth="445.0" text="GameOver" textFill="#1a0a0a">
         <font>
            <Font name="System Bold Italic" size="72.0" />
         </font>
      </Label>
      <Button layoutX="870.0" layoutY="437.0" mnemonicParsing="false" onAction="#onRetryClicked" prefHeight="60.0" prefWidth="135.0" text="Retry">
         <stylesheets>
            <URL value="@Cartoon_UI.css" />
            <URL value="@style1.css" />
         </stylesheets>
         <styleClass>
            <String fx:value="button-cartoon" />
            <String fx:value="zoom-effect" />
         </styleClass>
      </Button>
      <Button layoutX="870.0" layoutY="556.0" mnemonicParsing="false" onAction="#onMenuClicked" prefHeight="60.0" prefWidth="135.0" text="Menu">
         <stylesheets>
            <URL value="@Cartoon_UI.css" />
            <URL value="@style1.css" />
         </stylesheets>
         <styleClass>
            <String fx:value="button-cartoon" />
            <String fx:value="zoom-effect" />
         </styleClass>
      </Button>
      <Button layoutX="871.0" layoutY="667.0" mnemonicParsing="false" onAction="#onExitClicked" prefHeight="60.0" prefWidth="135.0" text="Exit">
         <stylesheets>
            <URL value="@Cartoon_UI.css" />
            <URL value="@style1.css" />
         </stylesheets>
         <styleClass>
            <String fx:value="button-cartoon" />
            <String fx:value="zoom-effect" />
         </styleClass>
      </Button>
   </children>
</AnchorPane>
